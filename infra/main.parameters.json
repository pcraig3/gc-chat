{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION=canadacentral}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "openAiDeploymentName": {
      "value": "${AZURE_OPENAI_DEPLOYMENT=gpt-4o}"
    },
    "openAiModelName": {
      "value": "${AZURE_OPENAI_MODEL=gpt-4o}"
    },
    "openAiModelVersion": {
      "value": "${AZURE_OPENAI_MODEL_VERSION=2024-11-20}"
    },
    "embeddingDeploymentName": {
      "value": "${AZURE_OPENAI_EMBEDDING_DEPLOYMENT=text-embedding-3-large}"
    },
    "embeddingModelName": {
      "value": "${AZURE_OPENAI_EMBEDDING_MODEL=text-embedding-3-large}"
    },
    "embeddingModelVersion": {
      "value": "${AZURE_OPENAI_EMBEDDING_MODEL_VERSION=1}"
    },
    "openAiResourceName": {
      "value": "${AZURE_OPENAI_RESOURCE}"
    },
    "openAiResourceGroupName": {
      "value": "${AZURE_OPENAI_RESOURCE_GROUP}"
    },
    "openAiResourceLocation": {
      "value": "${AZURE_OPENAI_RESOURCE_LOCATION=canadaeast}"
    },
    "openAiApiVersion": {
      "value": "${AZURE_OPENAI_API_VERSION}"
    },
    "createAzureOpenAi": {
      "value": "${CREATE_AZURE_OPENAI=true}"
    },
    "openAiKey": {
      "value": "${AZURE_OPENAI_KEY}"
    },
    "openAiEndpoint": {
      "value": "${AZURE_OPENAI_ENDPOINT}"
    },
    "createRoleForUser": {
      "value": "${CREATE_ROLE_FOR_USER=true}"
    },
    "acaExists": {
      "value": "${SERVICE_ACA_RESOURCE_EXISTS=false}"
    },
    "disableKeyBasedAuth": {
      "value": "${DISABLE_KEY_BASED_AUTH=false}"
    },
    "cosmosEnableFreeTier": {
      "value": "${COSMOS_ENABLE_FREE_TIER=false}"
    }
  }
}
